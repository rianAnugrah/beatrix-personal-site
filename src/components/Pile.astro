---
export interface Props {
  text: string;
  class?: string;
}

const { text, class: className } = Astro.props;
---

<p class={" text-nowrap text-center bg-white px-4 h-[2rem] flex items-center justify-center rounded-full px-leading-[86px] text-[#0A0614]" + className } style="font-family: 'Playfair Display', serif;">
    {text}
</p>